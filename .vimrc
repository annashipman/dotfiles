set nocompatible
syntax on

set number
set list "show invisible character
set showcmd
set expandtab "use spaces instead of tabs
set tabstop=2 softtabstop=2 shiftwidth=2 "default tab sizes
set ruler "show row and column at the bottom of the window


"noremap  <Up>      <NOP>
"inoremap  <Down>   <NOP>
"inoremap  <Left>   <NOP>
"inoremap  <Right>  <NOP>
"noremap   <Up>     <NOP>
"noremap   <Down>   <NOP>
"noremap   <Left>   <NOP>
"noremap   <Right>  <NOP>

source ~/.vim/anna.vim
